{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "16": "icons/icon16.png",
         "19": "icons/icon19.png",
         "32": "icons/icon32.png",
         "38": "icons/icon38.png",
         "48": "icons/icon48.png",
         "512": "icons/icon512.png",
         "64": "icons/icon64.png"
      },
      "default_popup": "popup.html",
      "default_title": "Binance Wallet"
   },
   "browser_specific_settings": {
      "gecko": {
         "id": "{0a395005-c941-4030-83c9-018ee43e3414}"
      }
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Alt+Shift+B",
            "linux": "Alt+Shift+B",
            "mac": "Alt+Shift+B",
            "windows": "Alt+Shift+B"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
