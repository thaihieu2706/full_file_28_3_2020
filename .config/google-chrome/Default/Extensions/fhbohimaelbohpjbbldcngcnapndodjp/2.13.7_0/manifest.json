   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "permissions": [ "tabs", "storage", "clipboardWrite", "identity" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.13.7"
}
